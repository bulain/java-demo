<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%t] %p %c{5}:%L - %m%n</pattern>
        </encoder>
    </appender>

    <logger name="com.bulain" level="DEBUG" />

    <root level="INFO">
        <appender-ref ref="console" />
    </root>

</configuration>  