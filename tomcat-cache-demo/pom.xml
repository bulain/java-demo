<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<groupId>com.bulain</groupId>
	<artifactId>tomcat-cache-demo</artifactId>
	<version>1.0.0</version>
	<packaging>war</packaging>
	<modelVersion>4.0.0</modelVersion>
	<name>tomcat-cache-demo</name>

	<properties>
		<javakaffee.version>2.1.1</javakaffee.version>
		<compiler.version>2.3.2</compiler.version>
		<eclipse.versions>2.9</eclipse.versions>
	</properties>

	<dependencies>

		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>memcached-session-manager</artifactId>
			<version>${javakaffee.version}</version>
		</dependency>
		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>memcached-session-manager-tc6</artifactId>
			<version>${javakaffee.version}</version>
		</dependency>
		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>memcached-session-manager-tc7</artifactId>
			<version>${javakaffee.version}</version>
		</dependency>
		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>memcached-session-manager-tc8</artifactId>
			<version>${javakaffee.version}</version>
		</dependency>
		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>msm-flexjson-serializer</artifactId>
			<version>${javakaffee.version}</version>
		</dependency>
		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>msm-xstream-serializer</artifactId>
			<version>${javakaffee.version}</version>
		</dependency>
		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>msm-javolution-serializer</artifactId>
			<version>${javakaffee.version}</version>
			<exclusions>
				<exclusion>
					<groupId>javolution</groupId>
					<artifactId>javolution</artifactId>
				</exclusion>
			</exclusions>
		</dependency>
		<dependency>
			<groupId>de.javakaffee.msm</groupId>
			<artifactId>msm-kryo-serializer</artifactId>
			<version>${javakaffee.version}</version>
		</dependency>
		<dependency>
			<groupId>javolution</groupId>
			<artifactId>javolution</artifactId>
			<version>5.4.5</version>
		</dependency>

	</dependencies>

	<build>
		<finalName>tomcat-cache-demo</finalName>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>${compiler.version}</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<encoding>UTF-8</encoding>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-eclipse-plugin</artifactId>
				<version>${eclipse.versions}</version>
			</plugin>
		</plugins>

	</build>

</project>
